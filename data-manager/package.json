{
  "name": "@formant/data-manager",
  "files": [
    "dist"
  ],
  "main": "./dist/formant-data-manager.umd.js",
  "module": "./dist/formant-data-manager.es.js",
  "exports": {
    ".": {
      "import": "./dist/formant-data-manager.es.js",
      "require": "./dist/formant-data-manager.umd.js"
    }
  },
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --port 4000",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "types": "tsc src/*.ts --lib ESNext,DOM --declaration --emitDeclarationOnly --outDir dist/types",
    "docs": "typedoc src/DataManager.ts --theme minimal --out ../docs/data-manager/"
  },
  "devDependencies": {
    "typedoc": "^0.21.9",
    "typescript": "^4.3.2",
    "vite": "^2.5.4"
  },
  "types": "./dist/types/main.d.ts"
}
